"VertexLitGeneric"
{
 
        "$basetexture" "models/gredwitch/common/prop"

	// "$alphatest"	"1"
	"$halflambert" "1"
	"$translucent" 	"1"
	"$nocull" 		"1"

        "$angle" 0.0
        "$translate" "[0.0 0.0]"
        "$center" "[0.5 0.5]"

        "Proxies"
        {
            "LinearRamp"
            {
                "rate" -945
                "initialValue" 0.0
                "resultVar" "$angle"
            }
            "TextureTransform"
            {
                "translateVar" "$translate"
                "rotateVar" "$angle"
                "centerVar" "$center"
                "resultVar" "$basetexturetransform"
            }
        }
    }